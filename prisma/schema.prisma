generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Country {
  name        String   @id @default(cuid())
  capital     String?
  region      String?
  population  Int?
  flag        String?
  currencies  String?
  estimated_gdp Decimal? @db.Decimal(20, 2)
}
